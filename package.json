{
  "name": "reusable-d3-charts",
  "version": "1.0.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/springload/reusable-d3-charts"
  },
  "engines": {
    "node": "^4.2.6"
  },
  "devDependencies": {
    "babel": "^6.3.26",
    "babel-core": "^6.4.5",
    "babel-eslint": "^4.1.1",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babelify": "^7.2.0",
    "browser-sync": "^2.8.0",
    "browserify": "^13.0.0",
    "browserify-incremental": "^3.0.0",
    "chai": "^3.2.0",
    "eslint": "^1.3.1",
    "eslint-config-airbnb": "^5.0.0",
    "eslint-plugin-react": "^3.14.0",
    "gulp": "^3.9.0",
    "gulp-inject": "^3.0.0",
    "gulp-parker": "^0.1.0",
    "gulp-pleeease": "^2.0.2",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^2.1.0",
    "gulp-sass-lint": "^1.1.0",
    "gulp-sourcemaps": "1.6.0",
    "gulp-svgmin": "^1.1.2",
    "gulp-svgstore": "^5.0.1",
    "gulp-uglify": "^1.4.0",
    "gulp-util": "^3.0.4",
    "isparta": "^4.0.0",
    "mocha": "^2.3.0",
    "mocha-generators": "^1.2.0",
    "nightmare": "^2.2.0",
    "sass-lint": "^1.4.0",
    "sinon": "^1.17.0",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0"
  },
  "dependencies": {
    "d3": "^3.5.14",
    "normalize.css": "^4.1.1",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "save-svg-as-png": "^1.0.2"
  },
  "scripts": {
    "start": "npm run watch -s",
    "linter:js": "eslint",
    "linter:js:ci": "eslint --no-eslintrc --config .ci-eslintrc",
    "linter:sass": "sass-lint -v",
    "lint:js": "npm run linter:js -s -- .",
    "lint:js:ci": "npm run linter:js:ci -s -- .",
    "lint:sass": "npm run linter:sass -s",
    "lint": "npm run lint:js -s && npm run lint:sass -s",
    "test": "npm run test:unit -s",
    "test:unit": "mocha --compilers js:babel-core/register client/js/tests/unit",
    "test:unit:watch": "mocha --watch --compilers js:babel-core/register client/js/tests/unit",
    "test:unit:coverage": "babel-node $(npm bin)/isparta cover node_modules/mocha/bin/_mocha -- client/js/tests/unit",
    "test:integration": "mocha --timeout 20000 --slow 1000 client/js/tests/integration",
    "test:ci": "./bin/ci-test.sh",
    "build": "NODE_ENV=development gulp build",
    "dist": "NODE_ENV=production gulp build",
    "css": "gulp css",
    "js": "gulp js",
    "watch": "gulp watch"
  }
}
